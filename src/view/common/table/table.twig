{% set uniqid = "tableau_id_" ~ "now"|date("Ymdhisu") ~ random(1, 9999999999999) %}
<table id="{{ uniqid }}" class="table_with_pagination">
    <thead>
        <tr>
            {% for column in columns %}
                <th scope="col">{{ column.label }}</th>
            {% endfor %}
        </tr>
    </thead>
    <tbody data-liste-url="{{ urlListe }}">
        <tr>
            <td colspan="{{ columns|length }}"></td>
        </tr>
    </tbody>
    {{ include('common/table/footer.twig') }}
</table>
<script>
$(window).load(function() {
    new Pagination(
        $("#{{ uniqid }}")
        ,'{{ "tableau_msg_clic_refresh" | lang | escape('js') }}'
    );
});
</script>
